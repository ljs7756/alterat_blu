@charset "utf-8";
/* CSS Document */

html, body,
h1, h2, h3, h4, h5, h6,
ul, ol, li, dl, dt, dd, div, p,
adress, span, em, strong, big, small, b, u, blockquote, pre,
table, abbr, caption, thead, tfoot, tbody, tr, th, td,
img, a, form, fieldset, legend, label, input, button, select, option, textarea { margin:0; padding:0; }
h1, h2, h3, h4, h5, h6 { font-size:1em; }
ul, ol, li { list-style:none; }
table { border-spacing:0; border-collapse:collapse; }
address,
em{ font-style:normal; }
img,
fieldset,
form { border:0; }
img { vertical-align:top; }
input,
select,
button { vertical-align:middle; }
hr { display:none; }
caption { visibility:hidden; overflow:hidden; font-size:1%; line-height:1%; }
legend,
.accessibility,
.hide { overflow:hidden; visibility:hidden; position:absolute; top:0; left:0; font-size:1%; line-height:1%; }
* html .png24 { tmp:expression(setPng24(this));} /* for IE */

html, body { font:normal 12px/1.5 돋움,dotum,arial,verdana; color:#d2d2d2; }
.left_bg { background:url(../images/left_bg.jpg)#262626 repeat-y; }
.bg_color { background:#262626; }

input, select { font:normal 12px/1.5 돋움,dotum,arial,verdana; color:#474747; }
select { _margin-top:-1px; border:1px solid #abadb3; }
.input_text { _margin-top:-1px; padding:0 0 0 5px; height:23px; line-height:23px; border:1px solid #ddd; }
input.radio,
input.check { _margin-top:-1px; margin-right:5px; width:13px; height:13px; }
input.file { _margin-top:-1px; width:100%; height:23px; line-height:23px; }
input.jfile1 {height:23px;line-height:23px;border:1px solid #ddd;}
textarea { overflow-x:hidden; overflow-y:scroll; padding:2px; font:normal 12px/1.5 돋움,dotum,arial,verdana; color:#474747; border:1px solid #ddd; }
a:link,
a:visited { text-decoration:none; color:#ccc; }
a:hover,
a:active { text-decoration:none; color:#FC0; }

.tl { text-align:left; }
.tc { text-align:center; }
/* left 없는 top영역 */
.left_no_top { width:100%; background:#262626; position:relative; border-top:3px solid #0099dd; border-bottom:2px solid #000; }
.top_leftno { width:1400px;}
.top_leftno:after { content:""; display:block; clear:both; }
.logo_area { float:left; width:280px; padding:18px 0 12px; background:#111;  text-align:center; }

.right_top { background:#262626; float:left;}
.right_top:after { content:""; display:block; clear:both; }
.right_top ul { float:left; width:910px;}
.right_top ul:after { content:""; display:block; clear:both; }
.right_top ul li.menu_on a { color:#FC0; }
.right_top ul li { float:left; font-size:16px; font-weight:bold; color:#CCC; border-right:2px solid #000; border-left:1px solid #444; }
.right_top ul li.rightno { float:left; font-size:16px; font-weight:bold; color:#CCC; border-right:none; border-left:1px solid #444; }
.right_top ul li a { display:block; padding:18px 30px 12px; }

.top_btns { text-align:right; float:right; }
.top_btns:after { content:""; display:block; clear:both; }
.top_btns p { float:left; border-right:2px solid #000; border-left:1px solid #444; color:#CCC;  }
.top_btns p a { display:block; padding:20px 20px 16px; background:#111; font-weight:bold; }

.find_file { border:none; padding:3px; background:#464646; width:300px; margin-bottom:30px; }

table { width:100%; }
table tr th { background:#464646; border:1px solid #555; padding:8px 0 8px 8px;}
table tr td { border:1px solid #555; padding:8px 0 8px 8px;}
table tr td.tc { text-align:center; }

/* 로그인 */
.login_bg { background:url(../images/login_bg.jpg) repeat; }
.login_wrap { width:545px; margin:300px auto 0; }
.login_wrap tr td { border:none; }

.input_bg { background:url(../images/login_input_bg.png) no-repeat; padding:24px 22px 24px 34px; height:196px; }
.login_id { width:296px; padding:8px 6px; font-size:14px; border-radius:5px; border:2px solid #00b2f2; background:url(../images/icon_id.jpg) #fff no-repeat 280px 50%; font-weight:bold; }
.login_pw { width:296px; padding:8px 6px; font-size:14px; border-radius:5px; border:2px solid #00b2f2; margin:15px 0; background:url(../images/icon_pw.jpg) #fff no-repeat 280px 50%; font-weight:bold; }

.section_wrap { width:100%; border-top:1px solid #666; background:#313131; padding:0 0 10px 0; position:relative;}
.section_wrap:after { content:""; display:block; clear:both; }

.section_wrap_l { width:100%; border-top:1px solid #666; background:url(../images/left_bg.jpg)#313131 repeat-y; padding:0 0 10px 0; position:relative; }
.section_wrap_l:after { content:""; display:block; clear:both; }

.left_list { float:left; width:280px;}
.left_list ul { margin-top:20px; }
.left_list ul li { padding:10px; font-size:14px; font-weight:bold; }
.left_list ul li a { color:#a9b1b9; }
.left_list ul li a:hover { color:#FC0; }

.right_wrap { margin-left:280px;}
.right_con { padding:30px 0 0 30px; width:1085px; }

.btn_add { clear:both; text-align:right; margin-top:20px; }
.btn_add a { display:inline-block; border:1px solid #999; background:#666; width:120px; height:30px; padding-top:10px; font-size:14px; font-weight:bold; color:#fff; margin:0 auto; text-align:center;}

.profile01 span { display:inline-block; width:177px; margin-right:50px; border-bottom:2px solid #01b1f0; font-size:16px; font-weight:bold; padding-bottom:8px; }
.profile2_w {margin:20px 0 0 0;}
.profile2_w:after { content:""; display:block; clear:both;}

.style_input { width:95%; padding:6px; font-size:14px;font-weight:bold; background:#292929; border:1px solid #666; color:#eee; }
.style_input01 { width:860px; padding:6px; font-size:14px; font-weight:bold; background:#292929; border:1px solid #666; color:#eee; }
select { padding:6px; font-size:14px; background:#292929; border:1px solid #666;  color:#fff;}

.profile2_left { float:left; width:177px; margin-right:50px; border-bottom:2px solid #01b1f0; font-size:16px; font-weight:bold; padding-bottom:8px; }
.profile2_left01 { width:177px; margin-right:50px; border-bottom:2px solid #01b1f0; font-size:16px; font-weight:bold; padding-bottom:8px;}

.chk_sty { border-radius:3px; border:none; }

.nps_tit { text-align:center; margin-bottom:20px; font-size:30px; font-weight:bold; color:#ddd; padding-bottom:10px; border-bottom:2px solid #0099dd; }

.graph_or { display:inline-block; width:75%; background:#dfdfdf; height:12px; position:relative; margin-right:10px; }
.graph_or span { background:#f79c00; position:absolute; left:0; top:1px; height:10px; }
.graph_or strong { font-size:14px; color:#456471; }
.graph_red { display:inline-block; width:75%; background:#dfdfdf; height:12px; position:relative; margin-right:10px; }
.graph_red span { background:#ff7847; position:absolute; left:0; top:1px; height:10px; }
.graph_red strong { font-size:14px; color:#456471; }
.graph_w { display:inline-block; width:60%; background:#dfdfdf; height:12px; position:relative; margin-right:10px; }
.graph_w span { background:#01b1f0; position:absolute; left:0; top:1px; height:10px; }
.graph_w strong { font-size:14px; color:#456471; }

.list_num { text-align:center; padding:30px 0; width:1100px; }
.list_num img { vertical-align:middle; }
.list_num a strong { display:inline-block; margin:0 6px; border-radius:3px; padding:5px 10px; border:1px solid #b7d4db; font-size:14px; }
.list_num a:hover strong { display:inline-block; margin:0 6px; border-radius:3px; padding:4px 9px; border:2px solid #65c2db; font-size:14px; }
.list_num a strong.now { display:inline-block; margin:0 6px; border-radius:3px; padding:4px 9px; border:2px solid #65c2db; font-size:14px; }

.list_num_1 { text-align:center; padding:30px 0; width:1400px; }
.list_num_1 img { vertical-align:middle; }
.list_num_1 a strong { display:inline-block; margin:0 6px; border-radius:3px; padding:5px 10px; border:1px solid #b7d4db; font-size:14px; }
.list_num_1 a:hover strong { display:inline-block; margin:0 6px; border-radius:3px; padding:4px 9px; border:2px solid #65c2db; font-size:14px; }
.list_num_1 a strong.now { display:inline-block; margin:0 6px; border-radius:3px; padding:4px 9px; border:2px solid #65c2db; font-size:14px; }

.nps_wrap { margin:30px; width:1370px; position:relative;}
.nps_wrap:after { content:""; display:block; clear:both; }
.nps_left { padding:2px; width:500px; height:810px; float:left; margin-bottom:30px; border:1px solid #555; }
.nps_right { padding:2px; width:700px; height:810px; float:left; margin:0 0 30px 20px; border:0px solid #555;}
.nps_metadata_wrap { border:none; background:#fff; height:480px; width:100%; }

.pop_wrap { position:absolute; top:137px; left:460px; width:750px; border:3px solid #01b1f0; background:#222; z-index:999; padding:15px; }
.file_info_txt { background:#313131; overflow:auto; padding:15px; height:500px; color:#fff; }

.footer_wrap { clear:both;  background:#262626; padding:30px 0 30px 30px;}

/* 팝업 */
.pop_system { display:block; position:absolute; top:62px; left:50%; margin-left:37px; z-index:999; background:#222; }
.pop_info { display:block; position:absolute; top:62px; left:50%; margin-left:37px; z-index:999; background:#222; width:430px; }
.pop_intro { display:block; position:absolute; top:62px; left:50%; margin-left:-36px; z-index:999; background:#222; }
.pop_wrap01 { padding:15px; border:1px solid #01b1f0; }
.pop_tit { font-weight:bold; font-size:16px; color:#f90; margin-bottom:20px; }
.pop_con { background:#585858; padding:20px; }
.pop_btns { padding:10px 0 0 0; text-align:center; }
.pop_btns span { display:inline-block; border-radius:5px; text-align:center; padding:8px 0 6px 0; color:#fff; font-size:13px; cursor:pointer; font-weight:bold; }